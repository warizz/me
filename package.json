{
  "name": "me",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:test": "turbo run build:test",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "gen-sitemap": "turbo run gen-sitemap",
    "lint": "turbo run lint",
    "start": "turbo run start",
    "start:test": "turbo run start:test",
    "test": "turbo run test",
    "test:ci": "turbo run test:ci",
    "tsc": "turbo run tsc"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "2.8.4",
    "turbo": "1.8.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "npm@9.2.0",
  "optionalDependencies": {
    "@next/swc-linux-x64-gnu": "13.2.1",
    "@next/swc-linux-x64-musl": "13.2.1",
    "turbo-linux-64": "1.8.6"
  }
}
