{
  "name": "me",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:test": "turbo run build:test",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:ci": "turbo run test:ci",
    "start": "turbo run start",
    "tsc": "turbo run tsc"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "2.8.1",
    "turbo": "1.6.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "npm@9.2.0"
}
