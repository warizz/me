{
  "name": "me",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "gen-sitemap": "turbo run gen-sitemap",
    "lint": "turbo run lint",
    "start": "turbo run start",
    "test": "turbo run test",
    "tsc": "turbo run tsc"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "3.5.0",
    "turbo": "2.4.1"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.4.0",
  "optionalDependencies": {
    "@next/swc-linux-x64-gnu": "15.1.7",
    "@next/swc-linux-x64-musl": "14.2.13",
    "turbo-linux-64": "2.4.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["cypress"]
  }
}
