{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["$NEXT_PUBLIC_IS_DISABLED_GA"],
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**"]
    },
    "build:test": {
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "tsc": {
      "outputs": []
    },
    "start": {
      "dependsOn": ["build"]
    },
    "start:test": {
      "dependsOn": ["build:test"]
    },
    "test": {},
    "test:ci": {}
  }
}
